allow-axfr-ips=0.0.0.0/0
api=yes
api-key=secret
default-api-rectify=yes
default-soa-edit=INCREMENT-WEEKS
default-publish-cdnskey=1
default-publish-cds=2,4
default-ttl=60
direct-dnskey=yes
setgid=pdns
setuid=pdns
webserver=yes
webserver-address=0.0.0.0
webserver-allow-from=0.0.0.0
webserver-max-bodysize=16
default-soa-content=get.desec.io. get.desec.io. 1 300 300 300 300

launch=gmysql
gmysql-host=authdb
gmysql-port=
gmysql-dbname=pdns
gmysql-user=pdns
gmysql-password=secret
gmysql-dnssec=yes

cache-ttl=0
negquery-cache-ttl=0
query-cache-ttl=0
